{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 2,
        "no-cond-assign": 2,
        "no-console": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 2,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unreachable": 2,
        "use-isnan": 1,
        "valid-jsdoc": 1,
        "valid-typeof": 1,
        "no-fallthrough": 1,
        "no-octal": 1,
        "no-redeclare": 2,
        "no-delete-var": 1,
        "no-undef": 2,
        "no-unused-vars": 1,
        "no-mixed-spaces-and-tabs": 1,
        "indent": ["warn", 4],
        "linebreak-style": ["warn", "unix"],
        "quotes": ["warn", "single"],
        "semi": ["warn", "always"],
        "block-scoped-var": ["error"],
        "complexity": ["warn", 3],
        "max-depth": ["warn", 4],
        "max-nested-callbacks": ["warn", 3],
        "consistent-return": 2,
        "curly": ["warn", "all"],
        "default-case": 1,
        "dot-location": ["error", "property"],
        "no-alert": 1,
        "no-caller": 1,
        "no-else-return": 1,
        "no-eq-null": 1,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-implicit-coercion": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-invalid-this": 1,
        "no-new-wrappers": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-useless-call": 1,
        "no-warning-comments": 1,
        "radix": 1,
        "yoda": ["warn", "never"],
        "strict": 0,
        "no-catch-shadow": 2,
        "no-shadow": 1,
        "no-shadow-restricted-names": 2,
        "no-use-before-define": 2,
        "no-trailing-spaces": 1,
        "array-bracket-spacing": ["warn", "never"],
        "block-spacing": 1,
        "brace-style": 1,
        "camelcase": 1,
        "comma-spacing": ["warn", { "before": false, "after": true }],
        "comma-style": ["error", "last"],
        "key-spacing": ["warn", { "beforeColon": false, "afterColon": true }],
        "keyword-spacing": ["warn", { "before": true, "after": true }],
        "consistent-this": ["warn", "self", "that"],
        "computed-property-spacing": ["warn", "never"],
        "no-lonely-if": 1,
        "no-spaced-func": 2,
        "no-whitespace-before-property": 2,
        "object-curly-spacing": [1, "always"],
        "quote-props": ["warn", "consistent"],
        "semi-spacing": ["warn", { "before": false, "after": true }],
        "space-in-parens": ["warn", "never"],
        "space-infix-ops": 1,
        "space-unary-ops": [1, { "words": true, "nonwords": false }],
        "space-before-blocks": ["warn", "always"],
        "space-before-function-paren": ["warn", {"anonymous": "always", "named": "never"}],
        "spaced-comment": 1,
        "max-len": [0, 80],
        "new-cap": 1,
        "new-parens": 2,
        "arrow-parens": ["warn", "as-needed", {"requireForBlockBody": true}]
    },
    "globals": {
        "angular": true,
        "_": true,
        "describe": true,
        "it": true,
        "before": true,
        "$": true,
        "assert": true,
        "Plotly": true
    }
}